            <string>var left_pressed = false;
var right_pressed = false;
var confirm_pressed = false;
var exit_pressed = false;

left_pressed = keyboard_check_pressed(vk_left) || obj_controls.leftstick_left_pressed;
right_pressed = keyboard_check_pressed(vk_right) || obj_controls.leftstick_right_pressed;
confirm_pressed = keyboard_check_pressed(ord('A')) || gamepad_button_check_pressed(0, gp_face1);
exit_pressed = keyboard_check_pressed(vk_escape) || gamepad_button_check_pressed(0, gp_start);

sac_msg -= 0.01;
if (sac_msg &lt; 0)
sac_msg = 0;

if (sacrifice) {
    if (left_pressed &amp;&amp; selected &gt; 0) {
        audio_play_sound(snd_select, 0, false);
        selected--;
    }
    if (right_pressed &amp;&amp; selected &lt; ds_list_size(item_list) - 1) {
        audio_play_sound(snd_select, 0, false);
        selected++;
    }
    if (confirm_pressed) {
        audio_stop_sound(mus_sacrifice);
        audio_play_sound(snd_confirm, 0, false);
        sacrifices++;
        sac_msg = 2;
        sac_name = ds_list_find_value(item_title_list, selected);
        if (ds_list_find_value(item_list, selected) == 6) {
            instance_destroy();
            room_goto_next();
        }
        else {
            ds_list_delete(item_list, selected);
            ds_list_delete(item_title_list, selected);
            ds_list_delete(item_desc_list, selected);
            selected = 0;
            with (obj_door)
                event_user(0);
            sacrifice = false;
            obj_altar.burn = true;
            
            if (ds_list_empty(item_list)) {
                ds_list_add(item_list, 6);
                ds_list_add(item_title_list, "Your Life");
                ds_list_add(item_desc_list, "There's only one thing you have left to sacrifice...");
            }
        }
    }
}

if (exit_pressed) {
    audio_play_sound(snd_cancel, 0, false);
    instance_destroy();
    room_goto(rm_menu);
}
</string>
